<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".bus_go.IncheonGoActivity">

    <android.widget.Button
        android:id="@+id/btn_IC_go1"
        android:layout_width="301dp"
        android:layout_height="120dp"
        android:layout_margin="10dp"
        android:background="@drawable/mainbtn2"
        android:text="06:20(인천 문학경기장),        06:40(송내)"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.widget.Button
        android:id="@+id/back_button12"
        android:layout_width="301dp"
        android:layout_height="62dp"
        android:layout_margin="10dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/mainbtn2"
        android:text="뒤로가기"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_IC_go1" />
</androidx.constraintlayout.widget.ConstraintLayout>